{
  "name": "nurmuhammet/laravel-helpers",
  "description": "Laravel Helpers that make life a better place to live by saving time, energy & nerves",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "role": "Developer",
      "name": "Nurmuhammet Allanov",
      "email": "nurmuhammet@mail.com",
      "homepage": "http://nurmuhammet.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nurmuhammet\\Helpers\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0",
    "illuminate/routing": "^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nurmuhammet\\Helpers\\Providers\\HelpersServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}

